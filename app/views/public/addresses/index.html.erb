<h3>配送先登録/一覧</h3>
<%= form_with model: @address, url: addresses_path, method: :post do |f| %>
  <div class="form-group mb-3 row">
    <label for="PostCode" class="col-sm-3 col-form-label">郵便番号（ハイフンなし）</label>
    <div class="col-sm-9">
      <%= f.email_field :post_code, autofocus: true, autocomplete: "0000000" %>
    </div>
  </div>

  <div class="form-group mb-3 row">
    <label for="Address" class="col-sm-3 col-form-label">住所</label>
    <div class="col-sm-9">
      <%= f.email_field :address, autofocus: true, autocomplete: "東京都渋谷区代々木神園町0-0" %>
    </div>
  </div>

  <div class="form-group mb-3 row">
    <label for="Name" class="col-sm-3 col-form-label">宛名</label>
    <div class="col-sm-9">
      <%= f.email_field :name, autofocus: true, autocomplete: "令和道子" %>
    </div>
  </div>

  <div class="actions">
    <%= f.submit "新規登録", class: "btn btn-success" %>
  </div>
<% end %>